{
  "name": "jotbox-mongo-server",
  "version": "0.1.0",
  "private": true,
  "description": "MongoDB-based API server for Jotbox schema",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "bootstrap": "ts-node src/scripts/bootstrapMongo.ts",
    "generate-types": "ts-node src/scripts/generateTypes.ts",
    "seed-super-role-types": "ts-node src/scripts/seedSuperRoleTypes.ts",
    "seed-super-user-roles": "ts-node src/scripts/seedSuperUserRoles.ts"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.5.0",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.30",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}
